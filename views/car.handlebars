    <link rel="stylesheet" href="/styles/car.css">
    
    <div class="carTitle">
          <h1>Carrito de compras</h1>
     </div>

{{#each products}}
    <div class="carItem">
        <div class="carItem__description">
            <div class="carItem__descriptionImg">
                <img src="{{image}}" alt="">
            </div>
            <div class="carItem__descriptionInfo">
                <h1>{{name}}</h1>
                <h2>PlayStation 4</h2>
                <h3>Disponible</h3>
                <a class="carItem__descriptionInfoSave" href="#">Guardar para m√°s tarde</a>
                <a href="/tienda">Comparar con otros articulos</a>
            </div>
        </div>
        <div class="carItem__important">
            <h2>Precio</h2>
            <h1>${{price}}</h1>
            <h3>Cantidad: {{count}}</h3>
            <img data-id="{{_id}}" class="carItem__importantDelete" src="/recursos/basura.png" alt="">
        </div>
    </div>

{{/each}}
    <div class="carToPay">
        <a href="/pago"><button class="carToPay__btn">Proceder al pago</button></a>
    </div>

    <script src="/scripts/car.js"></script>
